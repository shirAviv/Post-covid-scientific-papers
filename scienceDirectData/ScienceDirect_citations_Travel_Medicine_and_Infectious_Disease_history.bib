@article{KUSKUCU2016572,
title = "Investigation of carbapenem resistance and the first identification of Klebsiella pneumoniae carbapenemase (KPC) enzyme among Escherichia coli isolates in Turkey: A prospective study",
journal = "Travel Medicine and Infectious Disease",
volume = "14",
number = "6",
pages = "572 - 576",
year = "2016",
note = "Including a Special Issue on Drug Resistant Pathogens and Travel",
