@article{ANTINORI201711,
title = "Is imported onchocerciasis a truly rare entity? Case report and review of the literature",
journal = "Travel Medicine and Infectious Disease",
volume = "16",
pages = "11 - 17",
year = "2017",
issn = "1477-8939",
doi = "https://doi.org/10.1016/j.tmaid.2017.02.006",
